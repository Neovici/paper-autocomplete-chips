{
	"name": "paper-autocomplete-chips",
	"version": "1.0.0",
	"description": "[![Build Status](https://travis-ci.org/Neovici/paper-autocomplete-chips.svg?branch=master)](https://travis-ci.org/Neovici/paper-autocomplete-chips)",
	"main": "index.js",
	"directories": {
		"test": "test"
	},
	"devDependencies": {
		"eslint": "^3.9.1",
		"eslint-plugin-html": "^3.2.1",
		"eslint-plugin-mocha": "^4.11.0",
		"polymer-cli": "^1.8.0",
		"web-component-tester-istanbul": "Neovici/web-component-tester-istanbul"
	},
	"scripts": {
		"start": "polymer serve -o",
		"preinstall": "rm -f package-lock.json",
		"postinstall": "polymer install --variants && yarn sync-npm",
		"lint": "eslint --cache --ext .js,.html . && polymer lint paper-*.html",
		"test": "polymer test",
		"analyze": "polymer analyze --input cosmoz-*.html  > analysis.json",
		"sync-npm": "npm shrinkwrap && mv npm-shrinkwrap.json package-lock.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/paper-autocomplete-chips.git"
	},
	"keywords": [
		"polymer",
		"web-components"
	],
	"author": "",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/neovici/paper-autocomplete-chips/issues"
	},
	"homepage": "https://github.com/neovici/paper-autocomplete-chips#readme"
}
